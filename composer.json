{
    "name": "nubs/sensible-cli",
    "description": "A symfony/console command wrapper around nubs/sensible.",
    "keywords": ["sensible", "script", "path", "executable", "editor", "browser", "pager"],
    "authors": [
        {
            "name": "Spencer Rinehart",
            "email": "anubis@overthemonkey.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "~5.4 || ~7.0",
        "nubs/sensible": "~0.4.0",
        "nubs/which": "~1.0",
        "symfony/console": "~2.5",
        "symfony/process": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "satooshi/php-coveralls": "~0.6.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "bin": [
        "bin/sensible"
    ],
    "autoload": {
        "psr-4": {
            "Nubs\\Sensible\\": "src"
        }
    }
}
